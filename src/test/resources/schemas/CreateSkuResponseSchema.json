{
  "$schema": "http://json-schema.org/draft-07/schema",
  "type": "object",
  "additionalProperties": false,
  "required": [
    "createdAt",
    "description",
    "price",
    "sku",
    "updatedAt"
  ],
  "properties": {
    "createdAt": {
      "type": "string",
      "pattern": "^\\d{10}\\.\\d+$"
    },
    "description": {
      "type": "string"
    },
    "price": {
      "type": "number"
    },
    "sku": {
      "type": "string"
    },
    "updatedAt": {
      "type": "string",
      "pattern": "^\\d{10}\\.\\d+$"
    }
  }
}